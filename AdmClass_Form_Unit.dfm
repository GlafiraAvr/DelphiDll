  object s_rights_upd: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  ID,'
      '  NAME_R,'
      '  UR_READ,'
      '  UR_ZAV,'
      '  UR_RASK,'
      '  UR_ZADV,'
      '  UR_POTER,'
      '  UR_NARAD,'
      '  UR_ADMIN,'
      '  UR_DEL'
      'from s_rights '
      'where'
      '  ID = :ID')
    ModifySQL.Strings = (
      'update s_rights'
      'set'
      '  NAME_R = :NAME_R,'
      '  UR_ADMIN = :UR_ADMIN,'
      '  UR_DEL = :UR_DEL,'
      '  UR_NARAD = :UR_NARAD,'
      '  UR_POTER = :UR_POTER,'
      '  UR_RASK = :UR_RASK,'
      '  UR_READ = :UR_READ,'
      '  UR_ZADV = :UR_ZADV,'
      '  UR_ZAV = :UR_ZAV'
      'where'
      '  ID = :ID')
    InsertSQL.Strings = (
      'insert into s_rights'
      
        '  (NAME_R, UR_ADMIN, UR_DEL, UR_NARAD, UR_POTER, UR_RASK, UR_REA' +
        'D, UR_ZADV, '
      '   UR_ZAV)'
      'values'
      
        '  (:NAME_R, :UR_ADMIN, :UR_DEL, :UR_NARAD, :UR_POTER, :UR_RASK, ' +
        ':UR_READ, '
